<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>ContactUsWebApp</display-name>
    <!-- Define the servlet -->
    <servlet>
        <servlet-name>ContactUsServlet</servlet-name>
        <servlet-class>ContactUsServlet</servlet-class>
    </servlet>

    <!-- Map the servlet to a URL pattern -->
    <servlet-mapping>
        <servlet-name>ContactUsServlet</servlet-name>
        <url-pattern>/ContactUsServlet</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>contact_us.jsp</welcome-file>
    </welcome-file-list>
</web-app>